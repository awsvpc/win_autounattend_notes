                <SynchronousCommand wcm:action="add">
                    <Order>1</Order>
                    <Description>Run PackerBuild Config script</Description>
                    <CommandLine>cmd.exe /c C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe -File a:\PackerBuild_Config.ps1</CommandLine>
